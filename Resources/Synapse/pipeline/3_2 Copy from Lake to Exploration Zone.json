{
	"name": "3_2 Copy from Lake to Exploration Zone",
	"properties": {
		"activities": [
			{
				"name": "Initative-based Copy from Datalake to Exploration Zone",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"modifiedDatetimeStart": {
								"value": "@pipeline().parameters.LastUTCWaterMark",
								"type": "Expression"
							}
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "AzureBlobStorage_Binary_NoFilename_Azure",
						"type": "DatasetReference",
						"parameters": {
							"StorageAccountContainerName": {
								"value": "@pipeline().parameters.StorageAccountContainerName",
								"type": "Expression"
							},
							"RelativePath": {
								"value": "@Concat(pipeline().parameters.SourceSystem,'/',pipeline().parameters.SourceTable)",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "AzureBlobStorage_Binary_NoFilename_Azure",
						"type": "DatasetReference",
						"parameters": {
							"StorageAccountContainerName": "exploration",
							"RelativePath": {
								"value": "@concat(if(empty(pipeline().parameters.ExplorationPath),concat(pipeline().parameters.ExplorationZoneInitiative,'\\',pipeline().parameters.SourceSystem,'\\',pipeline().parameters.SourceTable),concat(pipeline().parameters.ExplorationPath,'\\',pipeline().parameters.SourceTable)))",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"ExplorationZoneInitiative": {
				"type": "string"
			},
			"SourceSystem": {
				"type": "string"
			},
			"SourceTable": {
				"type": "string"
			},
			"LastUTCWaterMark": {
				"type": "string"
			},
			"ExplorationPath": {
				"type": "string"
			},
			"StorageAccountContainerName": {
				"type": "string"
			}
		},
		"folder": {
			"name": "ZZ Pl Template/9 Copy to Zone Pipelines"
		},
		"annotations": []
	}
}